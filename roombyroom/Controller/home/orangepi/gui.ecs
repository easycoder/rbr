!   test.ecs

    script Test

    file File
    text Exit
    variable Pyctures

    debug step

    create screen
!        size 800 600
        fullscreen
        fill `#ffc`

    open File `json/test.json` for reading
    read Pyctures from File
    close File
    print Pyctures
    render Pyctures

    attach Exit to `exit`
    on click Exit
    begin
        close screen
        exit
    end

    show screen
    exit
