{
    "#doc": "The statistics UI",
    "#element": "div",
    "background": "white",
    "border": "1px solid gray",
    "margin-top": "0.5em",
    "padding": "0 0.5em 0.5e 0.5em",
    "box-shadow": "0.5em 0.5em 0.5em #444",
    "text-align": "center",
    "#": ["$Data", "$OKButton"],

    "$Data": {
        "#element": "div",
        "@id": "statistics-data"
    },

    "$OKButton": {
        "#element": "button",
        "@id": "statistics-ok",
        "#content": "OK",
        "margin": "1em 0",
        "width": "50%"
    }
}
